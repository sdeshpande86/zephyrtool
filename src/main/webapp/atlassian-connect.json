{
     "name": "AppD Zephyr Test View Addon",
     "description": "This plugin gives a hierarchial view of all issueType=Test in Zephyr project",
     "key": "appdynamics-zephyr",
     "baseUrl": "https://zephyr.engg.appdynamics.com",
     "vendor": {
         "name": "AppDynamics, Inc.",
         "url": "http://www.appdynamics.com"
     },
     "authentication": {
         "type": "jwt"
     },
     "lifecycle": {
        "installed": "/zephyrtool/rest/zephyrcallback"
     },
     "apiVersion": 1,
     "modules": {
         "generalPages": [
             {
                 "url": "/zephyrtool/index.html",
                 "key": "index",
                 "location": "system.top.navigation.bar",
                 "name": {
                     "value": "Tests View"
                 }
             }
         ]
     },
     "scopes": [
        "read",
        "write"
     ]
 }
